{
  "name": "dmp-ai-assistant-poc",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "compile": "tsc",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:show": "npm run typeorm migration:show -- -d ./dist/config/data-source.config.js",
    "migration:run": "npm run typeorm migration:run -- -d ./dist/config/data-source.config.js",
    "migration:generate": "npm run typeorm -- -d ./src/config/data-source.config.ts migration:generate ./src/migrations/$npm_config_name",
    "migration:create": "npm run typeorm -- migration:create ./src/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm -- -d ./dist/config/data-source.config.js migration:revert",
    "start": "ts-node --files -H -T ./src/index.ts",
    "start:watch": "nodemon -w *.ts -e ts -x ts-node --files -H -T ./src/index.ts",
    "start:wait-debug": "nodemon -w *.ts -e ts -x ts-node --files -H -T ./src/index.ts --inspect-brk=0.0.0.0:9229",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "format:write": "prettier --write \"src/**/*.ts\"",
    "lint:check": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint --fix \"{src,test}/**/*.ts\"",
    "seed": "node dist/seed.js"
  },
  "dependencies": {
    "@node-saml/passport-saml": "^4.0.0",
    "ajv": "^8.17.1",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "connect-ensure-login": "^0.1.1",
    "connect-typeorm": "^2.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^4.20.0",
    "express-session": "^1.18.2",
    "json-schema-to-ts": "^3.1.1",
    "morgan": "^1.10.0",
    "mysql2": "^3.14.0",
    "node-cache": "^5.1.2",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "passport-saml-metadata": "^5.0.0",
    "pdf-parse": "^1.1.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rollbar": "^2.26.4",
    "typeorm": "^0.3.22",
    "ws": "^8.18.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/bcrypt": "^5.0.2",
    "@types/connect-ensure-login": "^0.1.9",
    "@types/cors": "^2.8.17",
    "@types/date-fns": "^2.5.3",
    "@types/express": "^4.0.0",
    "@types/express-session": "^1.18.2",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.17",
    "@types/passport": "^1.0.9",
    "@types/passport-local": "^1.0.38",
    "@types/pdf-parse": "^1.1.5",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^8.0.0",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^7.0.0"
  },
  "volta": {
    "node": "20.19.4",
    "npm": "10.8.2"
  }
}
