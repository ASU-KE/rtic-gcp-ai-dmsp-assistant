{
  "name": "dmp-ai-assistant-poc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && npx tsc",
    "start": "node dist/src/index.js",
    "start-watch1": "npx tsc --watch && nodemon dist/src/index.js --inspect=0.0.0.0:9229",
    "start-watch": "npx concurrently \"npx tsc --watch\" \"nodemon --watch dist --ext js --exec 'node dist/src/index.js'\"",
    "start-wait-debugger": "nodemon src/index.js --inspect-brk=0.0.0.0:9229",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "dependencies": {
    "ajv": "8.12.0",
    "cors": "2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "jsonwebtoken": "9.0.0",
    "morgan": "1.10.0",
    "mysql2": "^2.1.0",
    "node-cache": "^5.1.2",
    "pdf-parse": "^1.1.1",
    "rollbar": "^2.26.4",
    "sequelize": "6.31.1",
    "sqlite3": "5.1.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.17",
    "@types/pdf-parse": "^1.1.5",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.2",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
